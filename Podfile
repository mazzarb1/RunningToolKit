# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'RunningToolkit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RunningToolkit

pod ‘FBSDKCoreKit’
pod ‘FBSDKShareKit’
pod ‘FBSDKLoginKit’
pod ‘Firebase’
pod ‘Firebase/Auth’
pod ‘GoogleSignIn’

  target 'RunningToolkitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RunningToolkitUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
